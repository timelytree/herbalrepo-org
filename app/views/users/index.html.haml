.wrapper#usersINDEX
  .console
    = render 'admin/nav'
    .toolbar
      .header ALL USERS
      .buttons
        %a{class: 'toolbarB', href: '/admin/user/new'} New User
    .content
      %ul.list
        - @users.each do |user|
          %li.item
            %a{href: "/admin/category/edit/#{user.slug}", class: 'itemLINK'}= user.name
